{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "class-methods-use-this": "off",
        "no-plusplus": "off",
        "strict": "off",
        "no-use-before-define": "off",
        "no-shadow": "off",
        "no-param-reassign": "off"
    },
}
